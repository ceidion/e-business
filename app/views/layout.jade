    // Header
    .header.header-sticky

      // Topbar 
      .topbar
        .container
          // Topbar Navigation 
          ul.loginbar.pull-right
            li.languagesSelector
              i.fa.fa-globe
              | 
              a 言語選択
              ul.languages
                li.active
                  a(href='#') 日本語
                    i.fa.fa-check
                li
                  a(href='#') 中国語
                li
                  a(href='#') 英語
            li.topbar-devider
            li
              a(href='login') ログイン


      // Navbar 
      .navbar.navbar-default.mega-menu(role='navigation')
        .container
          // Brand
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-responsive-collapse')
              span.sr-only Toggle navigation
              span.fa.fa-bars
            a.navbar-brand(href='index.html')
              //- img#logo-header(src='img/logo1-default.png', alt='Logo')
              img#logo-header(src='img/eb/logo.gif', alt='Logo', style="height: 39px; width: auto;")
              //- h1 E-Business Inc.
          // Collect the nav links
          .collapse.navbar-collapse.navbar-responsive-collapse
            ul.nav.navbar-nav
              li.active
                a(href='home')
                  | Home
              li
                a(href='about-us')
                  | 会社概要  
              li
                a(href='services')
                  | 事業内容
              li
                a(href='recruitment')
                  | 採用情報
              li
                a(href='contact')
                  | お問合せ

